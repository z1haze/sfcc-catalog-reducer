<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <custom-type type-id="CatalogReducerDefinition">
        <display-name xml:lang="x-default">Catalog Reducer Definition</display-name>
        <description xml:lang="x-default">A definition for a catalog reduction, specifying necessary elements of the reduction job</description>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>site</storage-scope>
        <key-definition attribute-id="ID">
            <display-name xml:lang="x-default">Definition ID</display-name>
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="exportImages">
                <display-name xml:lang="x-default">Export images</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="imageSizes">
                <display-name xml:lang="x-default">Image Sizes</display-name>
                <description xml:lang="x-default">List of image sizes to export with products</description>
                <type>text</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="masterCatalogs">
                <display-name xml:lang="x-default">Master Catalogs</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="maxProductsPerCategory">
                <display-name xml:lang="x-default">Products Per Category</display-name>
                <description xml:lang="x-default">The maximum number of products to include per category, not including variations</description>
                <type>int</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="maxVariantsPerMaster">
                <display-name xml:lang="x-default">Variations Per Master</display-name>
                <description xml:lang="x-default">The maximum number of variation products to include per each master product</description>
                <type>int</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="onlineProducts">
                <display-name xml:lang="x-default">Online Products Only</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="orderableProducts">
                <display-name xml:lang="x-default">Orderable Products Only</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="productIDs">
                <display-name xml:lang="x-default">Product IDs</display-name>
                <description xml:lang="x-default">List of product IDs to export</description>
                <type>text</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="state">
                <display-name xml:lang="x-default">State</display-name>
                <description xml:lang="x-default">Current state of what is happening in the process</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="storefrontCatalog">
                <display-name xml:lang="x-default">Storefront Catalog</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="exportPricebooks">
                <display-name xml:lang="x-default">Should pricebooks be exported?</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="exportInventoryList">
                <display-name xml:lang="x-default">Should the inventory-list be exported?</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="presentation">
                <display-name xml:lang="x-default">Presentation</display-name>
                <attribute attribute-id="maxProductsPerCategory"/>
                <attribute attribute-id="maxVariantsPerMaster"/>
                <attribute attribute-id="creationDate" system="true"/>
                <attribute attribute-id="lastModified" system="true"/>
                <attribute attribute-id="ID"/>
                <attribute attribute-id="onlineProducts"/>
                <attribute attribute-id="orderableProducts"/>
                <attribute attribute-id="productIDs"/>
                <attribute attribute-id="exportImages"/>
                <attribute attribute-id="imageSizes"/>
                <attribute attribute-id="exportPricebooks"/>
                <attribute attribute-id="exportInventoryList"/>
                <attribute attribute-id="storefrontCatalog"/>
                <attribute attribute-id="masterCatalogs"/>
                <attribute attribute-id="state"/>
            </attribute-group>
        </group-definitions>
    </custom-type>
</metadata>
