							</tr>
							<tr>
								<td>Online Products Only &#09;</td>
								<td><input name="online" id="online" type="checkbox" checked></td>
								<td></td>
							</tr>
						</table>
					</td>
				</tr>
				
			<iselseif condition="${pdict.CREMenu == 'Manual'}">
			
				<tr>
					<td>Manual</td>
				</tr>
				
			<iselse>
			
				<tr>
					<td>CSV</td>
				</tr>
				
			</isif>
		</tbody>
	</table>